{
  "name": "@konfry/typescript-fundamentals",
  "version": "1.0.10",
  "description": "Fundamental typescript types and concepts for a robust and easy to work system",
  "index": "dist/result.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "test": "vitest"
  },
  "keywords": [
    "typescript",
    "library",
    "result",
    "utility"
  ],
  "author": "Konrad Frysiak <konrad.frysiak@gmail.com>",
  "exports": {
    "./result": {
      "types": "./dist/result.d.ts",
      "import": "./dist/result.mjs",
      "require": "./dist/result.js"
    }
  },
  "typesVersions": {
    "*": {
      "result": [
        "dist/result.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kfrysiak/typescript-fundamentals"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  }
}